			<script>
				var last_viewAddrListQuery = "";
				function viewAddrList(_req) {
					last_viewAddrListQuery = _req;
					
					table_AddrlistChecked = new Array();
					cnt_AddrChecked = 0;
					
					$('#addrlist-search').blur();
					$("#area_addrlist").html('<div style="text-align:center;color:gray;padding-top:30px;padding-bottom:30px;"><div class="loader">Loading...</div></div>');
					
					$.ajax({
					    url: "index._addrlist.php",
					    type: "GET",
					    data: ""+_req+"&t="+(new Date()).getTime()+"&nopost=1",
					    timeout: 20000,
					    cache: false
					}).done(function(data, status, xhr) {
						if (data.indexOf("[OK]") != -1) {
							$("#area_addrlist").html(data).trigger( "create" );
							
							if (data.indexOf("[REQUEST_MAXPAGE]") != -1) {
								request_maxpage = true;
							}
						}
					}).fail(function(xhr, status, error) {

					});
				}
				$(function(){
					setTimeout(function(){viewAddrList("")}, 100);
				});
				
					//ページ数取得しなかった場合はここまでスクロールした時点でajaxで取得要求
					var request_maxpage = false;
					$(function(){
						$(window).scroll(function(){
							checkScrollRequestMaxpage();
						});

						function checkScrollRequestMaxpage() {
							if (!request_maxpage) return;
							var t = $('#area-addrlist-pageing-message').offset().top;
							var p = t - $(window).height();
							if ($(window).scrollTop() > p) {
								request_maxpage = false;
								
								$.ajax({
								    url: "index._addrlist.php",
								    type: "GET",
								    data: ""+last_viewAddrListQuery+"&action=requestMaxPage&nopost=1",
								    timeout: 20000,
								    cache: false
								}).done(function(data, status, xhr) {
									var _ent = parseValue(data);
									setMaxPage(_ent["maxpage"], _ent["listcnt"]);
								}).fail(function(xhr, status, error) {
								    
								});
							}
						}
						checkScrollRequestMaxpage();
					});
				
					function toggleMailAddrData(_tar, _addr) {
						//if ($("#area_maildata_"+_tar).css("display") == "block") {
						//	$("#area_maildata_"+_tar).css({"display":"none"});
						//	$("#area_maildata_"+_tar).popup('close');
						//} else {
							openMailAddrData(_tar, _addr);
						//}
					}
				
					function openMailAddrData(_tar, _addr) {
						$("#area_maildata_"+_tar).popup('close');
						$("#area_maildata_"+_tar).css({"display":"block"});
							
						if ($("#area_maildata_"+_tar).html().indexOf("[OK]") != -1) {
							$("#area_maildata_"+_tar).popup('open', {positionTo: "window"});
							return;
						}
						$("#area_maildata_"+_tar).html('<div style="text-align:center;padding:20px;"><div class="loader">Loading...</div></div>');
						
						$.ajax({
						    url: "index._addrlist_addr.php",
						    type: "GET",
						    data: "&UID_enc=aaAmpWgi1vgOex38ZRE410y%2BtXd%2BZx6NhK6lvaQK8ItEpoVILu8KcnF1wzVRcUzQ&address="+encodeURIComponent(_addr)+"&t="+(new Date()).getTime()+"&nopost=1",
						    timeout: 20000,
						    cache: false
						}).done(function(data, status, xhr) {
							if (data.indexOf("[OK]") != -1) {
								$("#area_maildata_"+_tar).html(data).trigger( "create" );
								$("#area_maildata_"+_tar).popup('open', {positionTo: "window"});
							} else {

							}
						}).fail(function(xhr, status, error) {
							
						});
					}
				
				var table_AddrlistChecked = new Array();
				var cnt_AddrChecked = 0;
				function checkToggleAddrlist(_tar) {
					if (table_AddrlistChecked[""+_tar] == true) {
						$("#area_mail_"+_tar+"_checkicon").removeClass("ui-icon-check").addClass("ui-icon-noncheck");
						table_AddrlistChecked[""+_tar] = false;
						cnt_AddrChecked --;
					} else {
						$("#area_mail_"+_tar+"_checkicon").removeClass("ui-icon-noncheck").addClass("ui-icon-check");
						table_AddrlistChecked[""+_tar] = true;
						cnt_AddrChecked ++;
					}
					

					if (cnt_AddrChecked > 0) {
						//$("#area_checked_buttons").css({display: "block"});
						$("#area_checked_buttons2").css({display: "block"});
					} else {
						//$("#area_checked_buttons").css({display: "none"});
						$("#area_checked_buttons2").css({display: "none"});
					}

				}
				
				var mailnumlist = "";
				function checkToggleAddrlistAll() {
					var _list = mailnumlist.split(",");
					var checked = 0;
					for (var n=0; n<_list.length; n++) {
						var _tar = _list[n];
						if (table_AddrlistChecked[""+_tar] == true) {
							checked ++;
						}
					}
					
					for (var n=0; n<_list.length; n++) {
						var _tar = _list[n];
						if (checked == _list.length) {
						} else {
							if (table_AddrlistChecked[""+_tar] == true) continue;
						}
						checkToggleAddrlist(_tar);
					}
				}
				
				function deleteFromChecked() {
					if(confirm('您確定要刪除所有選定的地址（##1大小寫）嗎？（使用該地址發送和接收的郵件不會被刪除）'.split("##1").join(cnt_AddrChecked))){
						var _tmplist = "";
						for (i in table_AddrlistChecked) {
							if (table_AddrlistChecked[i]) {
								_tmplist = ""+i+","+_tmplist;
							}
						}
					
						$.ajax({
						    url: "index._addrlist.php",
						    type: "GET",
						    data: "action=delAddrList&nopost=1&num_list="+encodeURIComponent(_tmplist),
						    timeout: 10000,
						    cache: false
						}).done(function(data, status, xhr) {
							//if (data == "OK") {
								viewAddrList("");
							//} else {
						 	   //alert(data);
							//}
						}).fail(function(xhr, status, error) {
						    alert("發生系統錯誤，請再試一次。");
						});
					}
				}
				
				function togglePushFromChecked() {
					var _tmplist = "";
					for (i in table_AddrlistChecked) {
						if (table_AddrlistChecked[i]) {
							_tmplist = ""+i+","+_tmplist;
						}
					}
				
					$.ajax({
					    url: "index._addrlist.php",
					    type: "GET",
					    data: "action=togglePushAddrList&nopost=1&num_list="+encodeURIComponent(_tmplist),
					    timeout: 10000,
					    cache: false
					}).done(function(data, status, xhr) {
						//if (data == "OK") {
							viewAddrList("");
						//} else {
					 	   //alert(data);
						//}
					}).fail(function(xhr, status, error) {
					    alert("操作失敗，請在互聯網連接穩定的地方再次嘗試。");
					});
				}
				
				function viewEditAddrAlias(_tar, _addr) {
					if ($("#area_addralias_"+_tar+"_input").css("display") == "block") {
						var _alias = $("#area_addralias_"+_tar+"_value").val();
						
						$.ajax({
						    url: "smphone.app.aliaslist.php",
						    type: "GET",
						    data: "action=updateAliasEditOnce&nopost=1&UID_enc=aaAmpWgi1vgOex38ZRE410y%2BtXd%2BZx6NhK6lvaQK8ItEpoVILu8KcnF1wzVRcUzQ&addr="+encodeURIComponent(_addr)+"&alias="+encodeURIComponent(_alias),
						    timeout: 10000,
						    cache: false
						}).done(function(data, status, xhr) {
							var _newname = ""+_addr;
							if (_alias != "") {
								_newname = ""+_alias+" ("+_addr+")";
							}
							//$("#area_mailhead_"+_tar+" > A").html(""+_newname);
							$("#area_mailaddr_"+_tar+"").html(""+_newname);
						}).fail(function(xhr, status, error) {
						    alert("操作失敗，請在互聯網連接穩定的地方再次嘗試。");
						});
						
						var _alias_view = _alias;
						if (_alias == "") { _alias_view = "（沒有設定）"; }
						$("#area_addralias_"+_tar+"_view").html("別號: "+_alias_view);
						
						$("#area_addralias_"+_tar+"_input").css({'display': 'none'});
						$("#area_addralias_"+_tar+"_view").css({'display': 'block'});
					} else {
						$("#area_addralias_"+_tar+"_view").css({'display': 'none'});
						$("#area_addralias_"+_tar+"_input").css({'display': 'block'});
					}
				}
				
				

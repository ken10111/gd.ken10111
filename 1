							<script>
								function requestLoginOAuth(_service) {
									closeLoginForm();
									
									openAlertDialog('<div class="loader">Loading...</div>');
									
									$.ajax({
									    url: "oauth_server.php",
									    type: "POST",
									    data: "action=requestLogin&service="+_service+"&by_app=&nopost=1&UID_enc=aaAmpWgi1vgOex38ZRE410y%2BtXd%2BZx6NhK6lvaQK8ItEpoVILu8KcnF1wzVRcUzQ&csrf_token_check=7193b5315ac214c3165ff5a13e6c50c2&csrf_subtoken_check=8ead9793c457b9c4a2b0cfb0f831fc39",
									    timeout: 5000,
									    cache: false
									}).done(function(data, status, xhr) {
										var _res = parseValue(data);
										if (_res["result"] == "OK") {
																							location.href = ""+decodeURIComponent(_res["url"]);
																							
											setTimeout(function(){closeAlertDialog();}, 1000);
										} else if (_res["msg"]) {
											alert(""+_res["msg"]);
										} else {
									 	   alert("發生系統錯誤，請再試一次。");
										}
									}).fail(function(xhr, status, error) {
									    alert("操作失敗，請在互聯網連接穩定的地方再次嘗試。");
									});
								}
							</script>
						



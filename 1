function confirmLogout() {
	$("#area_logindata").popup('close');
	
	openConfirmDialog(
		'註銷會為您提供完全不同的帳戶和電子郵件地址。如果您想恢復以前的地址和電子郵件，則需要使用相同的ID和密碼登錄。\n\n·如果您沒有ID和密碼，而您註銷，則無法再次恢復當前地址和郵件。\n·請務必註銷以防止在使用共享智能手機時他人濫用。', 
		function(){
			openAlertDialog('<div class="loader">Loading...</div>');
			singleLogout();
		},
		function() {
			openLoginData();
		}
	);
}
	function singleLogout() {
		$.ajax({
		    url: "/index.php",
		    type: "GET",
		    data: "action=singleLogout&nopost=1&no_uid=1&UID_enc=aaAmpWgi1vgOex38ZRE410y%2BtXd%2BZx6NhK6lvaQK8ItEpoVILu8KcnF1wzVRcUzQ&csrf_token_check=7193b5315ac214c3165ff5a13e6c50c2&csrf_subtoken_check=8ead9793c457b9c4a2b0cfb0f831fc39",
		    timeout: 5000,
		    cache: false
		}).done(function(data, status, xhr) {
			var _res = parseValue(data);
			if (_res["result"] == "OK") {
				FlexCall("action=changeUID;new_UID="+_res["UID"]+";");
			} else if (_res["msg"]) {
		 	   openAlertDialog(_res["msg"]);
			} else {
				openAlertDialog("發生系統錯誤，請再試一次。");
			}
		}).fail(function(xhr, status, error) {
		    openAlertDialog("操作失敗，請在互聯網連接穩定的地方再次嘗試。");
		});
	}

</script>


	
	

<script>
var errorSlotFlag = false;
function getSlotCheck() {
	if (errorSlotFlag) {
		alert("使用本功能時，請關閉廣告封鎖器。很抱歉造成您的困擾，顯示廣告是為了賺取維持網站營運的費用，感謝您的配合。");
		return false;
	}
	
	return true;
}
</script>
	<div id="ad_blocker_bait" style="visibility:visible;" class="pub_300x250 pub_300x250m pub_728x90 text-ad textAd text_ad text_ads text-ads text-ad-links">&nbsp;</div>
	<script>
	setTimeout(function(){
		var bait = $('#ad_blocker_bait');
		if (bait.css('display') === 'none' || bait.css('visibility') === 'hidden') {
			errorSlotFlag = true;
		}
	}, 100);
	</script>


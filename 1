	<script>
		function toggleMenu() {
			if ($(".layer").css("display") == "none") {
				openMenu();
			} else {
				closeMenu();
			}
		}
	    function openMenu(){
	    	$(".layer").show();
			$("#area-master").animate({'left' : 300 }, 300);
			$("#slide_menu").animate({'left' : 0 }, 300);
		}
	    function closeMenu(){
	    	$(".layer").hide();
			$("#area-master").animate({'left' : 0 }, 300);
			$("#slide_menu").animate({'left' : -300 }, 300);
		}
	</script>


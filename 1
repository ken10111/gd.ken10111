function checkManualMailAddr() {
	if (!getSlotCheck()) {
		return false;
	}
	
	$.ajax({
	    url: "/index.php",
	    type: "GET",
	    data: "action=checkNewMailUser&ip=61.239.212.232&nopost=1&UID_enc=aaAmpWgi1vgOex38ZRE410y%2BtXd%2BZx6NhK6lvaQK8ItEpoVILu8KcnF1wzVRcUzQ&csrf_token_check=7193b5315ac214c3165ff5a13e6c50c2&csrf_subtoken_check=8ead9793c457b9c4a2b0cfb0f831fc39&newdomain="+encodeURIComponent($(':radio[name="input_manualmaildomain"]:checked').val())+"&newuser="+encodeURIComponent($("#input_manualmailaddr").val()),
	    timeout: 10000,
	    cache: false
	}).done(function(data, status, xhr) {
		if (data == "OK") {
			//チェックがOKなので作成する
			$.ajax({
			    url: "/index.php",
			    type: "GET",
			    data: "action=addMailAddrByManual&nopost=1&by_system=1&ip=61.239.212.232&t=1610187758&UID_enc=aaAmpWgi1vgOex38ZRE410y%2BtXd%2BZx6NhK6lvaQK8ItEpoVILu8KcnF1wzVRcUzQ&csrf_token_check=7193b5315ac214c3165ff5a13e6c50c2&newdomain="+encodeURIComponent($(':radio[name="input_manualmaildomain"]:checked').val())+"&newuser="+encodeURIComponent($("#input_manualmailaddr").val())+"&recaptcha_token="+recaptcha_token,
			    timeout: 10000,
			    cache: false
			}).done(function(data, status, xhr) {
				if (data.indexOf("OK:") != -1) {
					//作成完了
					var _tmp = data.split("OK:");
					
											CookieQuery("newaddress_view="+_tmp[1]);
						location.href='/index.php';
									} else {
			 	   alert(data);
				}
			}).fail(function(xhr, status, error) {
			    alert("操作失敗，請在互聯網連接穩定的地方再次嘗試。");
			});
		} else if (data.indexOf("OFFER:") == 0) {
			var _tmp = data.split("OFFER:");
			if (_tmp[1]) {
				var _tmp = _tmp[1].split(",");
				if (_tmp[0] && _tmp[1]) {
					openConfirmDialog(
						'帳戶名稱“##1”已被使用且無法註冊，但如果添加“##2”並添加一個數字到末尾，就可以註冊。您確定要繼續並註冊此地址嗎？'.split("##1").join(_tmp[0]).split("##2").join(_tmp[1]+"@"+_tmp[2]), 
						function(){
							$("#input_manualmailaddr").val(''+_tmp[1]);
							checkManualMailAddr();
						},
						function() {
						}
					);
				}
			}
		} else {
	 	   alert(data);
		}
	}).fail(function(xhr, status, error) {
	    alert("操作失敗，請在互聯網連接穩定的地方再次嘗試。");
	});
}

function checkChangeUsernumber() {
	if ($("#new_number").val() == "") {
		alert("請輸入一個新ID。");
		return;
	}
	if ($("#new_number").val().length < 4) {
		alert("請輸入一個至少包含4個字母的新ID。");
		return;
	}
	
	$.ajax({
	    url: "/index.php",
	    type: "GET",
	    data: "action=changeUsernumber&nopost=1&UID_enc=aaAmpWgi1vgOex38ZRE410y%2BtXd%2BZx6NhK6lvaQK8ItEpoVILu8KcnF1wzVRcUzQ&csrf_token_check=7193b5315ac214c3165ff5a13e6c50c2&csrf_subtoken_check=8ead9793c457b9c4a2b0cfb0f831fc39&new_number="+encodeURIComponent($("#new_number").val()),
	    timeout: 10000,
	    cache: false
	}).done(function(data, status, xhr) {
		if (data == "OK") {
			$("#area_changenumber").popup('close');
			
			FlexCall("action=changeUserNumber;user_number="+$("#new_number").val()+";");
			
			$("#area_numberview").html(""+$("#new_number").val());
			$("#new_number").val("");
			
			$("#area_changenumber").css({"display": "none"});
		} else {
	 	   alert(data);
		}
	}).fail(function(xhr, status, error) {
	    alert("操作失敗，請在互聯網連接穩定的地方再次嘗試。");
	});
}

function checkChangePassword() {
	if ($("#new_password").val() == "") {
		alert("請輸入一個新密碼。");
		return;
	}
	if ($("#new_password_re").val() != $("#new_password").val()) {
		alert("新密碼和確認輸入不匹配，請輸入兩次相同的內容以防止輸入錯誤。");
		return;
	}
	if ($("#new_password_re").val().length < 4) {
		alert("請輸入一個至少包含4個字母的新密碼。");
		return;
	}
	
	$.ajax({
	    url: "/index.php",
	    type: "GET",
	    data: "action=changePassword&nopost=1&UID_enc=aaAmpWgi1vgOex38ZRE410y%2BtXd%2BZx6NhK6lvaQK8ItEpoVILu8KcnF1wzVRcUzQ&csrf_token_check=7193b5315ac214c3165ff5a13e6c50c2&csrf_subtoken_check=8ead9793c457b9c4a2b0cfb0f831fc39&new_password="+encodeURIComponent($("#new_password").val()),
	    timeout: 10000,
	    cache: false
	}).done(function(data, status, xhr) {
		if (data == "OK") {
			$("#area_changepassword").popup('close');
			
			$("#area_passwordview").html("（如果改變則不會顯示）");
			$("#new_password").val("");
			$("#new_password_re").val("");
			
			$("#area_changepassword").css({"display": "none"});
			//$("#area_changepassword_button").css({"display": "block"});
		} else {
	 	   alert(data);
		}
	}).fail(function(xhr, status, error) {
	    alert("操作失敗，請在互聯網連接穩定的地方再次嘗試。");
	});
}


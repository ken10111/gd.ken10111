function registCampaincode() {
	if ($("#campaign_code").val() == "") {
		alert("請輸入代碼。");
		return;
	}
	
	$.ajax({
	    url: "/index.php",
	    type: "GET",
	    data: "action=registCampaincode&nopost=1&UID_enc=aaAmpWgi1vgOex38ZRE410y%2BtXd%2BZx6NhK6lvaQK8ItEpoVILu8KcnF1wzVRcUzQ&code="+encodeURIComponent($("#campaign_code").val())+"&t="+(new Date()).getTime(),
	    timeout: 10000,
	    cache: false
	}).done(function(data, status, xhr) {
		if (data.indexOf("OK:") != -1) {
			var _tmp = data.split("OK:");
			if (_tmp.length >= 2) alert(_tmp[1]);
			
			location.href = "/index.php";
		} else if (data.indexOf("NG:") != -1) {
			var _tmp = data.split("NG:");
			if (_tmp.length >= 2) alert(_tmp[1]);
		} else {
	 	   //alert(data);
		}
	}).fail(function(xhr, status, error) {
	    alert("操作失敗，請在互聯網連接穩定的地方再次嘗試。");
	});
}

function checkLogin() {

	if ($("#user_number").val() == "") {
		alert("請輸入您的ID。");
		return;
	}
	if ($("#user_password").val() == "") {
		alert("請輸入你的密碼。");
		return;
	}
	
	$.ajax({
	    url: "/index.php",
	    type: "GET",
	    data: "action=checkLogin&nopost=1&UID_enc=aaAmpWgi1vgOex38ZRE410y%2BtXd%2BZx6NhK6lvaQK8ItEpoVILu8KcnF1wzVRcUzQ&csrf_token_check=7193b5315ac214c3165ff5a13e6c50c2&csrf_subtoken_check=8ead9793c457b9c4a2b0cfb0f831fc39&number="+encodeURIComponent($("#user_number").val())+"&password="+encodeURIComponent($("#user_password").val())+"&t="+(new Date()).getTime(),
	    timeout: 10000,
	    cache: false
	}).done(function(data, status, xhr) {
		if (data.indexOf("OK:") != -1) {
			var _tmp = data.split("OK:");
			FlexCall("action=changeUID;new_UID="+_tmp[1]+";");
		} else {
	 	   alert(data);
		}
	}).fail(function(xhr, status, error) {
	    alert("操作失敗，請在互聯網連接穩定的地方再次嘗試。");
	});
}

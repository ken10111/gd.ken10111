<script>
function updateAddressPushState(_address, _new, _addrkey) {
	if (_new == "1") {
		$("#area_mailaddr_disablepush_"+_addrkey).css("display", "inline");
	} else {
		$("#area_mailaddr_disablepush_"+_addrkey).css("display", "none");
	}
	
	$.ajax({
	    url: "/index.php",
	    type: "POST",
	    data: "action=updateAddressPushState&nopost=1&UID_enc=aaAmpWgi1vgOex38ZRE410y%2BtXd%2BZx6NhK6lvaQK8ItEpoVILu8KcnF1wzVRcUzQ&csrf_token_check=7193b5315ac214c3165ff5a13e6c50c2&csrf_subtoken_check=8ead9793c457b9c4a2b0cfb0f831fc39&address="+encodeURIComponent(_address)+"&push_disable="+encodeURIComponent(_new),
	    timeout: 10000
	}).done(function(data, status, xhr) {
	}).fail(function(xhr, status, error) {
	});
}

function deleteMailAddr(_mailaddr, _target, _type) {
	if (_type != "1") {
		openConfirmDialog(
			'如果您從電子郵件地址##1刪除，使用此地址發送/接收的郵件也將被刪除。 您確定要繼續嗎？如果您想保留郵件，請從“僅刪除地址”中刪除。）'.split("##1").join(_mailaddr), 
			function(){
				deleteMailAddrNow(_mailaddr, _target, _type);
			},
			function() {
			}
		);
	} else {
		openConfirmDialog(
			'您確定要刪除電子郵件地址##1嗎？ （使用此地址發送和接收的郵件不會被刪除。）'.split("##1").join(_mailaddr), 
			function(){
				deleteMailAddrNow(_mailaddr, _target, _type);
			},
			function() {
			}
		);
		

	}
}

	function deleteMailAddrNow(_mailaddr, _target, _type) {
		$.ajax({
		    url: "/index.php",
		    type: "GET",
		    data: "action=delMailAddr&nopost=1&UID_enc=aaAmpWgi1vgOex38ZRE410y%2BtXd%2BZx6NhK6lvaQK8ItEpoVILu8KcnF1wzVRcUzQ&csrf_token_check=7193b5315ac214c3165ff5a13e6c50c2&csrf_subtoken_check=8ead9793c457b9c4a2b0cfb0f831fc39&addr="+encodeURIComponent(_mailaddr)+"&type="+encodeURIComponent(_type),
		    timeout: 10000,
		    cache: false
		}).done(function(data, status, xhr) {
			if (data == "OK") {
				$("#area_mail_"+_target).css("display", "none");
				$("#area_maildata_"+_target).css("display", "none");
			} else if (data.indexOf("OK:") == 0) {
				$("#area_mail_"+_target).css("display", "none");
				$("#area_maildata_"+_target).css("display", "none");
				
				var _tmp = data.split("OK:");
				if (parseInt(_tmp[1]) <= 0) {
					alert("沒有地址，自動創建了新地址。");
					location.href='/index.php';
				} else {
					//window.scrollBy(0,0-200);
				}
			} else {
		 	   alert(data);
			}
		}).fail(function(xhr, status, error) {
		    alert("操作失敗，請在互聯網連接穩定的地方再次嘗試。");
		});
	}

function checkDetailAddr(_mailaddr, _target) {
	openAlertDialog('<div class="loader">Loading...</div>', function(){});
	
	$.ajax({
	    url: "/index.php",
	    type: "GET",
	    data: "action=checkDetailAddr&nopost=1&UID_enc=aaAmpWgi1vgOex38ZRE410y%2BtXd%2BZx6NhK6lvaQK8ItEpoVILu8KcnF1wzVRcUzQ&csrf_token_check=7193b5315ac214c3165ff5a13e6c50c2&csrf_subtoken_check=8ead9793c457b9c4a2b0cfb0f831fc39&addr="+encodeURIComponent(_mailaddr),
	    timeout: 10000,
	    cache: false
	}).done(function(data, status, xhr) {
		if (data.indexOf("#OK#") != -1) {
			//alert(""+data);
			//$("#"+_target).html(""+data);
			openAlertDialog(""+data, function(){});
		} else {
	 	   //alert(data);
		}
	}).fail(function(xhr, status, error) {
	    alert("操作失敗，請在互聯網連接穩定的地方再次嘗試。");
	});
}

function addMailAddrByAuto() {
	$.ajax({
	    url: "/index.php",
	    type: "GET",
	    data: "action=addMailAddrByAuto&nopost=1&by_system=1&UID_enc=aaAmpWgi1vgOex38ZRE410y%2BtXd%2BZx6NhK6lvaQK8ItEpoVILu8KcnF1wzVRcUzQ&csrf_token_check=7193b5315ac214c3165ff5a13e6c50c2&csrf_subtoken_check=8ead9793c457b9c4a2b0cfb0f831fc39&recaptcha_token="+recaptcha_token,
	    timeout: 10000,
	    cache: false
	}).done(function(data, status, xhr) {
		if (data.indexOf("OK:") != -1) {
			//作成完了
			var _tmp = data.split("OK:");
			
							CookieQuery("newaddress_view="+_tmp[1]);
				location.href='/index.php';
					} else {
	 	   alert(data);
		}
	}).fail(function(xhr, status, error) {
	    alert("操作失敗，請在互聯網連接穩定的地方再次嘗試。");
	});
}

